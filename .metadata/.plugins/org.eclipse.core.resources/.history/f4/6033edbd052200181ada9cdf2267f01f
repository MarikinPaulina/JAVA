package lab3;

import java.awt.BorderLayout;
import java.awt.GraphicsConfiguration;
import java.awt.HeadlessException;

import javax.swing.JComboBox;
import javax.swing.JFrame;

public class OuterClassListener extends JFrame {

	public OuterClassListener() throws HeadlessException {
		String[] colors = {"red", "green", "blue"};
		JComboBox<String> colorList = new JComboBox<String>(colors);
		this.add(colorList, BorderLayout.PAGE_START);
	}

	public OuterClassListener(GraphicsConfiguration gc) {
		super(gc);
		// TODO Auto-generated constructor stub
	}

	public OuterClassListener(String title) throws HeadlessException {
		super(title);
		// TODO Auto-generated constructor stub
	}

	public OuterClassListener(String title, GraphicsConfiguration gc) {
		super(title, gc);
		// TODO Auto-generated constructor stub
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
